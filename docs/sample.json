[
    {
        "start": "00:00:00.306",
        "end": "00:00:00.844",
        "text": "那一天",
        "text2": "あの日"
    },
    {
        "start": "00:00:01.280",
        "end": "00:00:02.220",
        "text": "星星降落的日子",
        "text2": "星が降った日"
    },
    {
        "start": "00:00:03.148",
        "end": "00:00:04.412",
        "text": "那简直是",
        "text2": "それはまるで"
    },
    {
        "start": "00:00:04.446",
        "end": "00:00:04.966",
        "text": "简直",
        "text2": "まるで"
    },
    {
        "start": "00:00:05.120",
        "end": "00:00:06.300",
        "text": "像梦中的景色一样",
        "text2": "夢の景色のように"
    },
    {
        "start": "00:00:07.438",
        "end": "00:00:08.978",
        "text": "美丽的景色",
        "text2": "美しい眺めだった"
    },
    {
        "start": "00:00:24.596",
        "end": "00:00:25.089",
        "text": "放学后",
        "text2": "放課後"
    },
    {
        "start": "00:00:25.705",
        "end": "00:00:26.261",
        "text": "不好",
        "text2": "悪い"
    },
    {
        "start": "00:00:26.475",
        "end": "00:00:27.679",
        "text": "我今天开始打工",
        "text2": "おれ今日これからバイト"
    },
    {
        "start": "00:00:29.345",
        "end": "00:00:31.689",
        "text": "已经讨厌这样的乡下了",
        "text2": "もうこんな田舎嫌だよー"
    },
    {
        "start": "00:00:31.969",
        "end": "00:00:33.527",
        "text": "讨厌这样的人生",
        "text2": "こんな人生嫌だ"
    },
    {
        "start": "00:00:33.927",
        "end": "00:00:37.970",
        "text": "来世请让我成为东京的帅哥吧",
        "text2": "来世は東京のイケメン男子にしてくださーい"
    },
    {
        "start": "00:00:42.083",
        "end": "00:00:42.874",
        "text": "哪里",
        "text2": "どこ"
    },
    {
        "start": "00:00:43.052",
        "end": "00:00:44.166",
        "text": "那么说来",
        "text2": "そういえば"
    },
    {
        "start": "00:00:44.554",
        "end": "00:00:47.078",
        "text": "我觉得好像一直做着奇怪的梦",
        "text2": "ずっと変な夢を見てたような気がするんだけど"
    },
    {
        "start": "00:00:48.324",
        "end": "00:00:50.626",
        "text": "别人的人生中…梦想",
        "text2": "別の人の人生の…夢"
    },
    {
        "start": "00:00:51.452",
        "end": "00:00:52.740",
        "text": "这是什么",
        "text2": "何だ これ"
    },
    {
        "start": "00:00:54.778",
        "end": "00:00:55.442",
        "text": "这个，这个",
        "text2": "これって"
    },
    {
        "start": "00:00:56.724",
        "end": "00:00:57.724",
        "text": "这难道是",
        "text2": "これってもしかして"
    },
    {
        "start": "00:00:58.076",
        "end": "00:00:59.328",
        "text": "我们在梦中",
        "text2": "俺達は夢の中で"
    },
    {
        "start": "00:01:00.136",
        "end": "00:01:01.202",
        "text": "替换着",
        "text2": "入れ替わってる"
    },
    {
        "start": "00:01:07.369",
        "end": "00:01:08.297",
        "text": "确实的事情",
        "text2": "確かなことが"
    },
    {
        "start": "00:01:08.757",
        "end": "00:01:09.541",
        "text": "只有一个",
        "text2": "一つだけある"
    },
    {
        "start": "00:01:10.325",
        "end": "00:01:11.037",
        "text": "我们是",
        "text2": "私達は"
    },
    {
        "start": "00:01:11.425",
        "end": "00:01:12.317",
        "text": "见面的话绝对",
        "text2": "会えば絶対"
    },
    {
        "start": "00:01:12.681",
        "end": "00:01:13.531",
        "text": "马上就明白",
        "text2": "すぐにわかる"
    },
    {
        "start": "00:01:14.255",
        "end": "00:01:15.279",
        "text": "我想说",
        "text2": "言おうと思ったんだ"
    },
    {
        "start": "00:01:16.075",
        "end": "00:01:17.579",
        "text": "你是世界的无论在哪里",
        "text2": "お前が世界のどこにいても"
    },
    {
        "start": "00:01:17.955",
        "end": "00:01:19.051",
        "text": "一定要去见你",
        "text2": "必ず会いに行くって"
    },
    {
        "start": "00:01:20.141",
        "end": "00:01:20.943",
        "text": "你是",
        "text2": "あなたは"
    },
    {
        "start": "00:01:21.361",
        "end": "00:01:21.803",
        "text": "谁",
        "text2": "誰"
    },
    {
        "start": "00:01:22.197",
        "end": "00:01:22.759",
        "text": "你啊",
        "text2": "お前は"
    },
    {
        "start": "00:01:23.483",
        "end": "00:01:24.039",
        "text": "是谁",
        "text2": "誰だ"
    },
    {
        "start": "00:01:25.693",
        "end": "00:01:26.465",
        "text": "你的名字是",
        "text2": "君の名は"
    }
]